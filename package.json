{
  "name": "linear",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "reactive-d3": "^1.0.1"
  },
  "scripts": {
    "start": "make -j all && react-scripts start",
    "build": "make -j all && react-scripts build",
    "test": "make -j all && react-scripts test",
    "deploy": "yarn run build && aws s3 sync build s3://zemn.me --acl public-read && yarn run deploy.invalidateCache",
    "deploy.invalidateCache": "./invalidate-cache.sh",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
